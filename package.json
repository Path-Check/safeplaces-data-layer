{
  "name": "@pathcheck/data-layer",
  "version": "0.1.1",
  "main": "index.js",
  "description": "Sample data layer for the Safeplaces backend API.",
  "repository": "git@github.com:Path-Check/safeplaces-data-layer.git",
  "author": "Jeffrey Hunter <jeff@jeffreyhunter.me>",
  "license": "MIT",
  "scripts": {
    "prebuild:prod": "rm -rf dist && mkdir dist",
    "build:prod": "cp -r src/* dist && cp -r bin dist && cp README.md ./dist",
    "postbuild:prod": "cp package.json dist/package.json",
    "migrate:up:private": "knex migrate:latest --knexfile src/config/private.js",
    "migrate:up:public": "knex migrate:latest --knexfile src/config/public.js",
    "migrate:down:private": "knex migrate:rollback --knexfile src/config/private.js",
    "migrate:down:public": "knex migrate:rollback --knexfile src/config/public.js",
    "seed:dev:private": "env NODE_ENV=development knex --knexfile src/config/private.js seed:run",
    "seed:test:private": "env NODE_ENV=test knex --knexfile src/config/private.js seed:run",
    "seed:dev:public": "env NODE_ENV=development knex --knexfile src/config/public.js seed:run",
    "seed:test:public": "env NODE_ENV=test knex --knexfile src/config/public.js seed:run",
    "lint": "eslint ./",
    "lint:fix": "eslint ./ --fix",
    "pretest": "npm run migrate:up:private && npm run migrate:up:public && npm run seed:test:private && npm run seed:test:public",
    "test": "nyc --reporter=html --reporter=text mocha test/**/*.* --exit",
    "posttest": "npm run seed:dev:private && npm run seed:dev:public",
    "test:slim": "mocha test/**/*.* --exit",
    "format": "prettier --write **/*.{js,json}"
  },
  "bin": {
    "spdl": "./bin/cli.js"
  },
  "engines": {
    "node": ">=12.14.1"
  },
  "pre-commit": [
    "format",
    "lint",
    "test"
  ],
  "dependencies": {
    "bcrypt": "^4.0.1",
    "bluebird": "^3.7.2",
    "chalk": "^4.1.0",
    "knex": "0.21.1",
    "knex-postgis": "0.12.0",
    "pg": "8.2.1",
    "wkx": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "babel-loader": "^8.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "hygen": "^5.0.3",
    "jshint": "^2.11.0",
    "mocha": "^7.1.1",
    "moment": "^2.27.0",
    "nyc": "^15.0.1",
    "pre-commit": "^1.2.2",
    "prettier": "^2.0.5",
    "random-coordinates": "^1.0.1",
    "sinon": "^9.0.2",
    "uuidv4": "^6.0.9",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
